{
  "name": "light-di",
  "version": "1.0.0",
  "description": "A simple, lightweight dependency injection container for JavaScript and TypeScript.",
  "type": "module",
  "main": "dist/light-di.js",
  "types": "dist/light-di.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/user/repo.git"
  },
  "scripts": {
    "build": "babel src --out-dir dist --extensions \".js\" --ignore \"src/**/*.test.js\" && tsc",
    "prepublishOnly": "npm run build",
    "test": "vitest --coverage",
    "example": "node main.js",
    "lint": "eslint src/",
    "format": "prettier --write .",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "dependency-injection",
    "di",
    "inversion-of-control",
    "ioc",
    "javascript",
    "typescript",
    "lightweight"
  ],
  "author": "Andrew Dunn",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@vitest/coverage-v8": "^3.2.4",
    "c8": "^10.1.3",
    "eslint": "^9.36.0",
    "prettier": "^3.6.2",
    "typescript": "^5.0.0",
    "vitepress": "^1.6.4",
    "vitest": "^3.2.4"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {}
  }
}
